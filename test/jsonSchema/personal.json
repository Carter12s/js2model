{
	"type":"object",
	"$schema": "http://json-schema.org/draft-04/schema",
	"required":false,
	"properties":{
		"address": {
			"$ref": "address.json"
		},
		"name": {
			"type":"object",
			"id": "http://jsonschema.net/name",
			"required":true,
			"properties":{
				"first": {
					"type":"string",
					"id": "http://jsonschema.net/name/first",
					"required":true
				},
				"last": {
					"type":"string",
					"id": "http://jsonschema.net/name/last",
					"required":true
				},
				"middle": {
					"type":"string",
					"id": "http://jsonschema.net/name/middle",
					"required":false
				}
			}
		},
		"phoneNumber": {
			"type":"array",
			"id": "http://jsonschema.net/phoneNumber",
			"required":false,
			"items":
				{
					"type":"object",
					"id": "http://jsonschema.net/phoneNumber/0",
					"required":false,
					"properties":{
						"number": {
							"type":"string",
							"id": "http://jsonschema.net/phoneNumber/0/number",
							"required":false
						},
						"type": {
							"type":"string",
							"id": "http://jsonschema.net/phoneNumber/0/type",
							"required":false
						}
					}
				}
			

		}
	}
}
