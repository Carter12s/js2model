{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "vehicleResult",
	"type": "object",
	"javaType": "com.thomsonreuters.lib.clear.data.VehicleResult",
	"javaInterfaces": ["java.io.Serializable"],
	"properties": {
		"vehicle": {
			"type": "object",
			"javaInterfaces": ["java.io.Serializable"],
			"properties": {
				"make": { "type": "string" },
				"model": { "type": "string" },
				"year": { "type": "string" },
				"vin": { "type": "string" },
				"style": { "type": "string" },
				"type": { "type": "string" }
			}
		},
		"plate": {
			"type": "object",
			"javaInterfaces": ["java.io.Serializable"],
			"properties": {
				"expirationDate": { "type": "string" },
				"plateNumber": { "type": "string" }
			}
		},
		"owner": {
			"type": "object",
			"javaInterfaces": ["java.io.Serializable"],
			"extends": {
				"$ref": "person.json"
			},
			"properties": {
				"address": { "$ref": "address.json" }
			}
		}
	}
}
